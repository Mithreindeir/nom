deposit = function(v):
	self.balance = self.balance + v
end

withdraw = function(v):
	self.balance = self.balance - v
end


new_acc = function(name):
	acc.name = name
	acc.balance = 0

	return acc
end

b = new_acc("John")
b.deposit = deposit
b.withdraw = withdraw
b.deposit(500)
b.withdraw(120)
print(b.balance, "\n")